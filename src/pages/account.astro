---
import Layout from "@/layouts/Layout.astro";
import AccountViewWithProvider from "@/components/account/AccountViewWithProvider";

export const prerender = false;

const user = Astro.locals.user;

if (!user) {
  return Astro.redirect("/auth/login?redirect=/account");
}
---

<Layout title="Moje konto - CashFlow Scenarios">
  <AccountViewWithProvider client:only="react" />
</Layout>
